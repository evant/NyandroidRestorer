<idea-plugin>
    <id>me.tatarka.nyandroid.NyandroidRestorer</id>
    <name>Nyandroid Restorer</name>
    <version>1.1</version>
    <vendor email="evan@tatarka.me" url="https://github.com/evant/NyandroidRestorer">NyandroidRestorer</vendor>

    <description><![CDATA[
      Restore our favorite pop-tart-rainbow friend in Android Studio
    ]]></description>

    <change-notes><![CDATA[
      <ul>
        <li> 1.1 - Update for more modern intellij versions </li>
        <li> 1.0 - Initial Release </li>
      </ul>
    ]]>
    </change-notes>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="162" />

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <!-- uncomment to enable plugin in all products
    -->
    <depends>com.intellij.modules.lang</depends>

    <extensions defaultExtensionNs="com.intellij">
        <postStartupActivity implementation="me.tatarka.nyandroid.NyandroidStartupActivity" />
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>
</idea-plugin>